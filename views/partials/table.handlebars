<table class="table table-dark">
  <thead>
    <tr>
      <th>#</th>
      <th>Titulo</th>
      <th>Precio</th>
      <th>Foto</th>
    </tr>
  </thead>
  <tbody>
    {{#each productos}}
    <tr>
      <td>
        {{ @index }}
      </td>
      <td>
        {{ this.title }}
      </td>
      <td>
        {{ this.price }}
      </td>
      <td>
        <img width="100" height="100" src="{{this.thumbnail}}" alt="{{this.name}}">
      </td>
    </tr>
    {{/each}}
  </tbody>
</table>